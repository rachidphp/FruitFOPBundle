{
    "name": "favouritefruit/fop-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle for Apache FOP",
    "keywords": ["Apache FOP", "pdf", "xsl", "xml", "fo"],
    "homepage": "https://github.com/favouritefruit/FruitFOPBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Contributors",
            "homepage": "https://github.com/favouritefruit/FruitFOPBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "favouritefruit/fop": "*",
        "knplabs/knp-gaufrette-bundle": "*"
    },
    "require-dev": {
        "symfony/yaml": "2.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "favouritefruit/fop",
                "source": {
                    "url": "git://github.com/iampersistent/FruitFOP.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Fruit\\FOPBundle": ""
        }
    },
    "target-dir" : "Fruit/FOPBundle"
}
